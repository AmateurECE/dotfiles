[user]
	email = ethan.twardy@gmail.com
	name = Ethan D. Twardy
[core]
	editor = /usr/bin/nano
	autocrlf = false
[init]
	defaultBranch = trunk
[pull]
	rebase = false
[grep]
	lineNumber = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
